{
    "manifest_version": 3,
    "name": "Astronomy Photo of the Day",
    "version": "1.0",
    "description": "View NASA's Astronomy Picture of the Day",
    "icons": {
      "128": "logo.png"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": "logo.png"
    },
    "permissions": [
      "activeTab",
      "scripting"
    ]
  }